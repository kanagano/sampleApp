<div class="container mt-4">
    <div class="row my-2">
        <div class="col-3 border-right">
            <%= render "search_form" %>
        </div>
        <div class="col-9">
            <% @museums.each do |museum| %>
                <%= render partial: "museum", locals: {museum: museum} %>
            <% end %>
        </div>
    </div>
</div>

<div class="container mb-5">
    <%= paginate @museums %>
</div>